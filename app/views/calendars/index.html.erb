

<h1>Index action-Listing calendars</h1>

<table>
  <tr>
    <th>Id</th>
    <th>User</th>
    <th>Event</th>
    <th>Event Location</th>
    <th>Event Time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @calendars.each do |calendar| %>
  <tr>
    <td><%= calendar.id %></td>
    <td><%= calendar.user_id %></td>
    <td><%= calendar.event %></td>
    <td><%= calendar.event_location %></td>
    <td><%= calendar.event_time %></td>
    <td><%= link_to 'Show', calendar %></td>
    <td><%= link_to 'Edit', edit_calendar_path(calendar) %></td>
    <td><%= link_to 'Destroy', calendar, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Calendar', new_calendar_path %>

