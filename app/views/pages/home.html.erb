
  <% if user_signed_in? %>
   <table summary="For signed-in users">
    <tr>
      <td >
        <%= render 'shared/calendar_form' %>
        <%= render 'shared/feed' %>
     </td>
    </tr>
  </table>
  <% else %>
     <%= link_to "Sign up", new_user_registration_path %> or
     <%= link_to "Sign in", new_user_session_path %>
  <% end %>

